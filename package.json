{
    "name": "desafio-cielo2",
    "version": "0.0.4",
    "description": "A starter project for AngularJS based on John Papa's style guide",
    "keywords": [
        "AngularJS",
        "angular",
        "template",
        "desafio-cielo2",
        "seed",
        "johnpapa",
        "styleguide",
        "node",
        "express"
    ],
    "homepage": "https://github.com/VitorHeser/DesafioCielo2",
    "bugs": {
        "url": "https://github.com/VitorHeser/DesafioCielo2"
    },
    "license": "MIT",
    "author": "Vitor Heser",
    "repository": {
        "type": "git",
        "url": "https://github.com/VitorHeser/DesafioCielo2.git"
    },
    "main": "server.js",
    "scripts": {
        "postinstall": "bower install",
        "prestart": "npm install ",
        "start": "concurrently \"json-server app/assets/data/lancamento-conta-legado.json\" \"node server.js\"",
        "pretest": "npm install",
        "test": "grunt karma:unit",
        "coveralls": "cat ./coverage/*/lcov.info | ./node_modules/.bin/coveralls && rm -rf ./coverage",
        "test-single-run": "grunt karma:singleRun",
        "preupdate-webdriver": "npm install",
        "update-webdriver": "webdriver-manager update",
        "preprotractor": "npm run update-webdriver",
        "protractor": "grunt protractor"
    },
    "dependencies": {
        "angular-chart.js": "^1.1.1",
        "bower": "^1.7.9",
        "chart.js": "^3.0.2",
        "express": "^4.14.0"
    },
    "devDependencies": {
        "bower": "^1.7.9",
        "coveralls": "^2.11.9",
        "grunt": "^1.0.1",
        "grunt-contrib-concat": "^1.0.1",
        "grunt-contrib-jshint": "^1.0.0",
        "grunt-contrib-uglify": "^1.0.1",
        "grunt-contrib-watch": "^1.0.0",
        "grunt-jscs": "^3.0.1",
        "grunt-karma": "^2.0.0",
        "grunt-protractor-runner": "^3.2.0",
        "jasmine-core": "^2.4.1",
        "jasmine-reporters": "^2.2.0",
        "karma": "^1.1.0",
        "karma-chrome-launcher": "^1.0.1",
        "karma-coverage": "^1.0.0",
        "karma-firefox-launcher": "^1.0.0",
        "karma-jasmine": "^1.0.2",
        "karma-junit-reporter": "^1.1.0",
        "karma-phantomjs-launcher": "^1.0.1",
        "load-grunt-tasks": "^3.5.0",
        "chart.js": "2.7.3",
        "phantomjs-prebuilt": "^2.1.7",
        "protractor": "^3.3.0",
        "json-server": "0.16.3",
        "concurrently": "6.0.1",
        "selenium-webdriver": "^2.53.2"
    },
    "engines": {
        "node": "^4.4.6",
        "npm": "^2.15.5"
    },
    "private": false
}
